<p id="notice"><%= notice %></p>

<h1>Listing Response Domain Datetimes</h1>

<table>
  <thead>
    <tr>
      <th>Datetime type</th>
      <th>Label</th>
      <th>Format</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_domain_datetimes.each do |response_domain_datetime| %>
      <tr>
        <td><%= response_domain_datetime.datetime_type %></td>
        <td><%= response_domain_datetime.label %></td>
        <td><%= response_domain_datetime.format %></td>
        <td><%= link_to 'Show', response_domain_datetime %></td>
        <td><%= link_to 'Edit', edit_response_domain_datetime_path(response_domain_datetime) %></td>
        <td><%= link_to 'Destroy', response_domain_datetime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response domain datetime', new_response_domain_datetime_path %>
