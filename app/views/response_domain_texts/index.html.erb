<p id="notice"><%= notice %></p>

<h1>Listing Response Domain Texts</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Maxlen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_domain_texts.each do |response_domain_text| %>
      <tr>
        <td><%= response_domain_text.label %></td>
        <td><%= response_domain_text.maxlen %></td>
        <td><%= link_to 'Show', response_domain_text %></td>
        <td><%= link_to 'Edit', edit_response_domain_text_path(response_domain_text) %></td>
        <td><%= link_to 'Destroy', response_domain_text, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response domain text', new_response_domain_text_path %>
