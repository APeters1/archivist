<p id="notice"><%= notice %></p>

<h1>Listing Response Domain Codes</h1>

<table>
  <thead>
    <tr>
      <th>Code list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_domain_codes.each do |response_domain_code| %>
      <tr>
        <td><%= response_domain_code.code_list %></td>
        <td><%= link_to 'Show', response_domain_code %></td>
        <td><%= link_to 'Edit', edit_response_domain_code_path(response_domain_code) %></td>
        <td><%= link_to 'Destroy', response_domain_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response domain code', new_response_domain_code_path %>
